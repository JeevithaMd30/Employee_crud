 @model CRUD_with_SP.Models.EmpModel

<!DOCTYPE html>
<html>

<body>
    <h3> Employee Data BY ID </h3>
    <div class="container mt-5">
        <form action="/Employee/EditPage" method="post" class="mt-3">
            <div class="form-group">
                <label for="Id">Id</label>
                <input asp-for="Id" class="form-control w-25" value="@Model.Id" />
            </div>
            <div class="form-group pt-2">
                <button type="submit" class="btn btn-primary">Submit</button>
            </div>
        </form>       
    </div>
   
</body>
</html>

